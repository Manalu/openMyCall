{
    "name": "phpMyCall",
    "authors": [
        "Ednei Leite da Silva <edsiddos@gmail.com>"
    ],
    "description": "Sistema de Helpdesk para controle de chamados desenvolvido em PHP",
    "main": "",
    "moduleType": [],
    "keywords": [
        "Helpdesk",
        "controle",
        "de",
        "chamados"
    ],
    "license": "GPLv3",
    "homepage": "",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "bootstrap": "~3.3.6",
        "font-awesome": "~4.5.0",
        "bootstrap-select": "~1.9.3",
        "bootstrap3-typeahead": "~4.0.0",
        "awesome-bootstrap-checkbox": "~0.3.5",
        "bootbox.js": "bootbox#~4.4.0",
        "angular": "~1.4.8",
        "bootstrap-datepicker": "~1.5.0"
    },
    "install": {
        "base": "static",
        "path": {
            "js": "{name}/js",
            "css": "{name}/css",
            "eot": "{name}/fonts",
            "otf": "{name}/fonts",
            "svg": "{name}/fonts",
            "ttf": "{name}/fonts",
            "woff": "{name}/fonts",
            "woff2": "{name}/fonts"
        },
        "sources": {
            "bootstrap": [
                "bower_components/bootstrap/dist/css/bootstrap.min.css",
                "bower_components/bootstrap/dist/js/bootstrap.min.js",
                "bower_components/bootstrap/dist/fonts/*"
            ],
            "font-awesome": [
                "bower_components/font-awesome/css/font-awesome.min.css",
                "bower_components/font-awesome/fonts/*"
            ],
            "awesome-bootstrap-checkbox": [
                "bower_components/awesome-bootstrap-checkbox.css"
            ],
            "bootstrap-select": [
                "bower_components/bootstrap-select/dist/css/bootstrap-select.min.css",
                "bower_components/bootstrap-select/dist/js/bootstrap-select.min.js",
                "bower_components/bootstrap-select/dist/js/i18n/*.min.js"
            ],
            "bootstrap3-typeahead": [
                "bower_components/bootstrap3-typeahead/bootstrap3-typeahead.min.js"
            ],
            "bootstrap-datepicker": [
                "bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker3.min.css",
                "bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker3.standalone.min.css",
                "bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
                "bower_components/bootstrap-datepicker/dist/locales/*.min.js"
            ]
        }
    }
}